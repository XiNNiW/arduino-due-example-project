#include <Arduino.h>
#include "factorial.h"
//The setup function is called once at startup of the sketch

int counter = 1;
void setup()
{
// Add your initialization code here
	Serial.begin(9600);
	Serial.print("Hello");

}

// The loop function is called in an endless loop
void loop()
{
	Serial.print("factorial of "+ counter + " is ...");
	Serial.print(factorial(counter));
	counter = (counter + 1)%5;
	delay(1000);
//Add your repeated code here
}
